{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "rescode": {
      "type": "string"
    },
    "message": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "body": {
          "type": "string"
        }
      },
      "required": [
        "title",
        "body"
      ]
    },
    "data": {
      "type": "object",
      "properties": {
        "access_token": {
          "type": "string"
        },
        "refresh_token": {
          "type": "string"
        }
      },
      "required": [
        "access_token",
        "refresh_token"
      ]
    }
  },
  "required": [
    "rescode",
    "message",
    "data"
  ]
}